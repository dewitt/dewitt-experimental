<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app 
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" 
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

 <display-name>Orchard Identity Server</display-name>

 <description>
  The Orchard Identity Server manages users.
 </description>

 <servlet>
  <servlet-name>HelloWorld</servlet-name>
  <servlet-class>net.unto.orchard.HelloWorldServlet</servlet-class>
 </servlet>

 <servlet-mapping>
  <servlet-name>HelloWorld</servlet-name>
  <url-pattern>/hello</url-pattern>
 </servlet-mapping>

</web-app>
