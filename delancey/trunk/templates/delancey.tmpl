  <link rel="stylesheet" type="text/css" 
        href="$site_info.project_url/static/css/delancey-0.1.css" />
  <script type="text/javascript" 
          src="$site_info.project_url/static/js/md5-2.2alpha.js"></script>
  <script type="text/javascript" 
          src="$site_info.project_url/static/js/prototype-1.4.0_rc4.js"></script>
  <script type="text/javascript"
          src="$site_info.project_url/static/js/delancey-0.1.js"></script>
  <script type="text/javascript"><!--
  var serverTime = [% server_time ? server_time : 0 %];
  var baseUrl = "$base_url";
  var delanceyClaim = '$delanceyClaim';
