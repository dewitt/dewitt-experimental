<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE html 
     PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
    "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">

<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
 <head>
  <meta content="text/html; charset=UTF-8" http-equiv="content-type" />
  <link rel="stylesheet" type="text/css" href="$site_info.project_url/static/css/spring.css" />
  <title>[% site_info.project_name FILTER ucfirst %] Search</title>
  <script type="text/javascript" src="$site_info.project_url/static/js/prototype-1.3.1.js"></script>
  <script type="text/javascript" src="$site_info.project_url/static/js/behaviour-1.0.js"></script>
  <script type="text/javascript" src="$site_info.project_url/static/js/spring-0.1.js"></script>
 </head>

 <body>

  <div class="page"> <!-- Page -->

   <div class="search-bar"> <!-- Search Command Line -->
    <span class="search-message"><a href="$site_info.project_url">[% site_info.project_name FILTER ucfirst %]</a> Search:</span>
    <form id="search-form" class="search-form" action="">
     <div>
      <input size="64" maxlength="80" id="search-text" value=""/>
      <div class="search-submit" id="search-submit">Go</div>
     </div>
    </form>
   </div>

   <div class="results"> <!-- Search Results -->

    <div class="tabs"> <!-- Tabs -->
     <ul class="tab-list"/>

     <div class="controls">
      <ul class="control-list">
       <li><a href="http://www.unto.net/unto/spring/">About Spring</a></li>
       <li><a href="http://www.unto.net/">Unto.net</a></li>
      </ul>
     </div>
    </div>

    </div>


    <div class="column-results"> <!-- Web Results -->

     <div class="notice" id="notice">
      Note: [% site_info.project_name FILTER ucfirst %] is a work in progress.  This page in particular is just a 
      placeholder during the development process.
     </div>
     
     <div class="column-header"> <!-- Web Results Header -->
      <span class="column-count"></span>
      <span class="column-name"></span>
     </div>

     <div class="items" id="items"> <!-- Items -->
     </div>

     <div class="column-paging"> <!-- Column Paging -->
      <ul class="link-list"/>
     </div>

     <div class="column-attribution"> <!-- Column Attribution -->
     </div>

     <div class="column-links"> <!-- Column Links -->
      <ul class="link-list"/>
     </div>

    </div>

   </div>

   <div class="footer"> <!-- Footer -->
   </div>

  </div>
 </body>
</html>
